                        <subviews>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" fixedFrame="YES" textAlignment="natural" translatesAutoresizingMaskIntoConstraints="NO" id="XFk-bp-mG5">
                                <rect key="frame" x="0.0" y="64" width="375" height="603"/>
